{
  "whiteSpace" : {
    "endl" : {
      "regex" : "\n",
      "flags" : ""
    }
  },
  "heading" : {
    "#" : {
      "regex" : "^#\\s",
      "flags" : "gm"
    },
    "##" : {
      "regex" : "^##\\s",
      "flags" : "gm"
    },
    "###" : {
      "regex" : "^###\\s",
      "flags" : "gm"
    },
    "####" : {
      "regex" : "^####\\s",
      "flags" : "gm"
    }
  },
  "inlineCode" : {
    "regex" : "`(?<codeString>.*?)`",
    "flags" : ""
  },
  "quote" : {

  },
  "list" : {

  },
  "tableRow" :{
    "regex" : "<tr>[\\s\\S]*?<\/tr>\n",
    "flags" : "gm"
  },
  "tableDivision" : {
    "regex" : "<tr>\n\t<td>:*-+[\\s\\S]+?<\/tr>",
    "flags" : "m"
  },
  "tableElement" : {
    "regex" : "\\|",
    "flags" : "g"
  },
  "link" : {
    "regex" : "[^!]\\[(?<linkName>.*?)\\]\\((?<linkSrc>.*?)\\)",
    "flags" : ""
  },
  "picture" : {
    "regex" : "!\\[(?<imageAltText>.*?)\\]\\((?<imageSrc>.*?)\\)",
    "flags" : ""
  },
  "bold-em" : {
    "regex" : "\\*{3}(?<boldEmContent>.*?)\\*{3}",
    "flags" : ""
  },
  "bold" : {
    "regex" : "\\*{2}(?<boldContent>.*?)\\*{2}",
    "flags" : ""
  },
  "em" : {
    "regex" : "\\*{1}(?<emContent>.*?)\\*{1}",
    "flags" : ""
  },
  "code-block" : {
    "regex":  "<pre><code class=\"language-[\\s\\S]*?\">(?<code>[\\s\\S]*?)<\\\/code><\\\/pre>",
    "flags" : "gm"
  },
  "code-start" : {
    "regex": "<pre><code class=\"language-[\\s\\S]*?\">",
    "flags": "gm"
  },
  "code-end" : {
    "regex" : "<\/code><\/pre>"
  },

  "content" : {
    "regex" : "<h1 id=\"h1-0\">[\\s\\S]*<\\/div>",
    "flags" : "gm"
  }
}